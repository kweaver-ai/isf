{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "description": "请求体结构定义",
    "required": [
        "resources",
        "method"
    ],
    "properties": {
        "resources": {
            "type": "array",
            "description": "资源列表",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    }
                }
            }
        },
        "method": {
            "type": "string",
            "description": "方法",
            "enum": [
                "GET"
            ]
        }
    }
}